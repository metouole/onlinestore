<h1>Shops#index</h1>
<p>Find me in app/views/shops/index.html.erb</p>
<p style="color: green"><%= notice %></p>

<h1>Products</h1>

<div id="products">
  <% @products.each do |product| %>
    <%= render product %>
    <p>
      <%= link_to "Show this product", shop_path(product) %>
    </p>
  <% end %>
</div>

